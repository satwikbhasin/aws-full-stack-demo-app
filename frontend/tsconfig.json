{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2015", "es2016", "esnext"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
  },
  "include": ["src/**/*"]
}